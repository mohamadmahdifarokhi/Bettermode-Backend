{
  "name": "bettermode-backend",
  "version": "1.0.0",
  "description": "Backend application for Bettermode's permission system",
  "main": "dist/main.js",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "test": "jest",
    "http": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/http.js\"",
    "test:e2e": "nyc --include '**/*.controller.ts' --check-coverage true ./node_modules/.bin/cucumber-js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "consumer": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/consumer.js\"",
    "migration:generate": "typeorm migration:generate -n",
    "migration:run": "typeorm migration:run",
    "migration:revert": "typeorm migration:revert"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.17.14",
    "@nestjs/apollo": "^10.2.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^10.2.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^9.0.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^9.0.0",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.8.0",
    "apollo-server-express": "^3.13.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "graphql": "^16.9.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "typeorm": "^0.3.10",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^7.3.2",
    "@cucumber/pretty-formatter": "^1.0.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.14.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/chai": "^5.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "chai": "^4.3.7",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^27.0.0",
    "nyc": "^17.1.0",
    "prettier": "^3.3.3",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.0",
    "typescript": "^4.9.5"
  }
}
